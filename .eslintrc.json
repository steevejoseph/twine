{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "import/prefer-default-export": "off"
  }
}
